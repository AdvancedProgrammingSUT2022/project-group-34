<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="app.views.MainMenuController"
            fx:id="pane"
            prefHeight="720.0" prefWidth="1280.0"
            stylesheets="@../css/main_menu.css">
    <Button layoutX="70" layoutY="200">Profile</Button>
    <Button layoutX="70" layoutY="300">Game Menu</Button>
    <Button layoutX="70" layoutY="400" id="logout">Logout</Button>
    <Button layoutX="960" layoutY="200">Leaderboard</Button>
    <Button layoutX="960" layoutY="300">Chat</Button>
    <Button layoutX="960" layoutY="400" id="exit" onMouseClicked="#exit">Exit</Button>

</Pane>
